<table class="table table-striped table-bordered table-hover">
	<tr>
		<th>Address Name</th>
		<th>Street</th>
		<th>Unit</th>
		<th>City</th>
		<th>State</th>
		<th>Zip</th>
	</tr>
	<tr>
		<td><%= @address.address_name %></td>
		<td><%= @address.contact_name %></td>
		<td><%= @address.street %></td>
		<td><%= @address.unit %></td>
		<td><%= @address.city %></td>
		<td><%= @address.state %></td>
		<td><%= @address.zipcode %></td>
	</tr>
</table>